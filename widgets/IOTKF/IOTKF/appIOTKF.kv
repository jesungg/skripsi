#:kivy 1.11

<welcomeScr>:
    BoxLayout:
        padding: [2,2,4,2]
        spacing: 2
        orientation: 'vertical'
        Label:
            font_size: 50
            halign: 'center'
            text: "Aplikasi Tracking Manusia \n Dalam Ruangan"
        Label:
            font_size: 30
            text: "...Details..."
        Button:
            pos_hint: {'center_x': .5, 'center_y': .5}
            size_hint: None, None
            size: 190,50
            text: "Mulai"
            on_press: root.manager.current = 'sel_vid_scr'
            on_release: 
        Button:
            pos_hint: {'center_x': .5, 'center_y': .5}
            size_hint: None, None
            size: 190,50
            text: "Bantuan"
            on_press: root.manager.current = 'help_scr'
        Button:
            pos_hint: {'center_x': .5, 'center_y': .5}
            size_hint: None, None
            size: 190,50
            text: "Tentang"
            on_press: root.manager.current = 'about_scr'
        Label:
            font_size: 10
            text: "...v0.0...dd/mm/yy..."

<selVidScr>:
    BoxLayout:
        padding: [2,2,2,10]
        spacing: 2
        orientation: 'vertical'

                    orientation: 'vertical'
                    id: containerr
            BoxLayout:
                padding: [2,2,2,2]
                spacing: 2
                orientation: 'vertical'
                id: container_gb
        Button:
            pos_hint: {'center_x': .5, 'center_y': .8}
            size_hint: None, None
            size: 150,20
            text: "Hapus"
            on_press: root.handle_delete()
        Button:
            pos_hint: {'center_x': .5, 'center_y': .8}
            size_hint: None, None
            size: 150,20
            text: "Selanjutnya"
            on_press: root.handleNext(root.manager)
        Button:
            pos_hint: {'center_x': .9, 'center_y': .9}
            size_hint: None, None
            size: 140,50
            size_hint: .1, .1
            text: "Kembali"
            on_press: root.manager.current = 'sel_vid_scr'

<SaveDialog>:
    on_pre_enter: text_input.clear_widgets()
    text_input: text_input
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserListView:
            id: savefilechooser
            on_selection: text_input.text = self.selection and self.selection[0] or ''

        TextInput:
            id: text_input
            size_hint_y: None
            height: 30
            multiline: False

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Batalkan"
                on_release: root.cancel()

            Button:
                text: "Simpan"
                on_release: root.save_plot(savefilechooser.path, text_input.text)

<resultScr>:
    on_pre_enter: root.lokasi_obj(),root.show_video()
    on_pre_leave: result_box.clear_widgets(),img_box.clear_widgets(),plot_box.clear_widgets()
    BoxLayout:
        padding: [2,2,2,2]
        spacing: 2
        orientation: 'vertical'
        BoxLayout:
            padding: [2,2,2,2]
            spacing: 2
            orientation: 'horizontal'
            BoxLayout:
                id:img_box
                padding: [2,2,2,2]
                spacing: 2
                orientation: 'horizontal'
                # Image:
                #     id: result_img
            BoxLayout:
                padding: [2,2,2,2]
                spacing: 2
                orientation: 'vertical'
                Label:
                    text:'Log Aktivitas'
                BoxLayout:
                    id: result_box
                    padding: [2,2,2,2]
                    spacing: 2
                    orientation: 'vertical'
        BoxLayout:
            padding: [2,2,2,2]
            spacing: 2
            orientation: 'horizontal'
            id: plot_box
        Button:
            pos_hint: {'center_x': .5}
            size_hint: None, None
            size: 150,20
            text: "Simpan grafik"
            on_press: root.show_save()
        Button:
            pos_hint: {'center_x': .5}
            size_hint: None, None
            size: 150,20
            text: "Selesai"
            on_press: root.manager.current = 'welcome_scr'
        Label:
            font_size: 10
            text: ""

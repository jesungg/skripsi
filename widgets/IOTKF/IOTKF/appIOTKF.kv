<chooseSet>:
    containerr : containerr
    container_gb : container_gb
    on_pre_enter: root.pulldata()
    BoxLayout:
        padding: [2,2,2,10]
        spacing: 2
        orientation: 'vertical'
        BoxLayout:
            padding: [2,2,2,10]
            spacing: 2
            orientation: 'horizontal'
            BoxLayout:
                padding: [2,2,2,10]
                spacing: 2
                orientation: 'vertical'
                id: containerr
            BoxLayout:
                padding: [2,2,2,10]
                spacing: 2
                orientation: 'vertical'
                id: container_gb
        Button:
            pos_hint: {'center_x': .5, 'center_y': .8}
            size_hint: None, None
            size: 150,20
            text: "Delete"
            on_press: root.handle_delete()
        Button:
            pos_hint: {'center_x': .5, 'center_y': .8}
            size_hint: None, None
            size: 150,20
            text: "Next"
            on_press: root.manager.current = 'result_scr'
        Label:
            font_size: 10
            text: "...v0.0...dd/mm/yy..."
<resultScr>:
